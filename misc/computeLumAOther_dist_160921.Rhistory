source("TCGA_BRCA_getPSN.R")
table(pheno$GROUP)
LumA <- pheno$ID[which(pheno$GROUP %in% "LumA")]
other <- pheno$ID[which(pheno$GROUP%in%"other")]
idx <- which(net[,1]%in%LumA & net[,2]%in% other)
idx2 <- which(net[,1]%in% other & net[,2]%in% LumA)
intersect(idx,idx2)
idx <- c(idx,idx2)
g <- igraph::graph_from_data_frame(d=net[idx,])
tmp <- igraph::shortest.paths(g,algorithm="dijkstra")
.getAvgD(tmp)
.getAvgD(d_overall)
